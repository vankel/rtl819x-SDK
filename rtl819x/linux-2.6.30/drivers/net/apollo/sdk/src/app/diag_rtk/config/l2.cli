//mac flush configuration
//unicast l2 entry
//include static entries
l2-table set flush mac-ucast include-static

//l2 table configuration
//set configuration
//mac flush configuration
//unicast l2 entry
//only static
l2-table set flush mac-ucast static-only

//l2 table configuration
//set configuration
//mac flush configuration
//unicast l2 entry
//port configuration
//port list, <0-6>
//all
//filter id configuration
//filter id, <0~15>
l2-table set flush mac-ucast port ( <PORT_LIST:ports> | all ) filter-id <UINT:fid>

//l2 table configuration
//set configuration
//mac flush configuration
//unicast l2 entry
//port configuration
//port list, <0-6>
//all
//filter id configuration
//filter id, <0~15>
//include static entries
l2-table set flush mac-ucast port ( <PORT_LIST:ports> | all ) filter-id <UINT:fid> include-static

//l2 table configuration
//set configuration
//mac flush configuration
//unicast l2 entry
//port configuration
//port list, <0-6>
//all
//filter id configuration
//filter id, <0~15>
//static entries only
l2-table set flush mac-ucast port ( <PORT_LIST:ports> | all ) filter-id <UINT:fid> static-only

//l2 table configuration
//set configuration
//mac flush configuration
//unicast l2 entry
//port configuration
//port list, <0-6>
