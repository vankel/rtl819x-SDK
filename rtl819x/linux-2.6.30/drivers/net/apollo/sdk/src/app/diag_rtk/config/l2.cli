//link down auto flush MAC table configuration
//state configuration
//disable
//enable
l2-table set link-down-flush state ( disable | enable )

//l2 table configuration
//set configuration
//lookup miss configuration
//multicast lookup
//trap priority
l2-table get lookup-miss multicast trap-priority

//l2 table configuration
//set configuration
//lookup miss configuration
//multicast lookup
//trap priority
//priority
l2-table set lookup-miss multicast trap-priority <UINT:priority>

//l2 table configuration
//get configuration
//lookup miss configuration
//broadcast lookup
//unicast lookup
//multicast lookup
//flooding port configuration
l2-table get lookup-miss ( broadcast | unicast | multicast ) flood-ports

//l2 table configuration
//set configuration
//lookup miss configuration
//broadcast lookup
//unicast lookup
//multicast lookup
//flooding port configuration
//flood port list, <0-6>
//all ports
l2-table set lookup-miss ( broadcast | unicast | multicast ) flood-ports ( <PORT_LIST:ports> | all )

//l2 table configuration
//get configuration
//lookup miss configuration
//port configuration
//port list, <0-6>
//all
//l2 multicast
//ipv4 muticast
//ipv6 muticast
//unicast l2
//action configuration
l2-table get lookup-miss port ( <PORT_LIST:ports> | all ) ( multicast | ip-mcast | ip6-mcast | unicast ) action

//l2 table configuration
//set configuration
//lookup miss configuration
//port configuration
//port list, <0-6>
//all
//ipv4 muticast
//ipv6 muticast
//action configuration
//drop
//flood in VLAN
//trap to cpu
l2-table set lookup-miss port ( <PORT_LIST:ports> | all ) ( ip-mcast | ip6-mcast ) action ( drop | flood-in-vlan | trap-to-cpu )

//l2 table configuration
//set configuration
//lookup miss configuration
//port configuration
//port list, <0-6>
//all
//l2 multicast
//action configuration
//drop
//drop exclude rma
//normal flooding
//trap to cpu
l2-table set lookup-miss port ( <PORT_LIST:ports> | all ) multicast action ( drop | drop-exclude-rma | flood-in-vlan | trap-to-cpu )

//l2 table configuration
//set configuration
//lookup miss configuration
//port configuration
//port list, <0-6>
//all
//unicast l2
//action configuration
//drop exclude IGMP/MLD
//forward
//trap to cpu exlcude IGMP/MLD
l2-table set lookup-miss port ( <PORT_LIST:ports> | all ) unicast action ( drop | flood-in-vlan | trap-to-cpu )

//l2 table configuration
//set configuration
//ipv4 multicast hash mode
//hash by dip and sip
//hash by dip and vid
//hash by vid and mac
l2-table set ip-mcast-mode ( dip-and-sip | dip-and-vid | vid-and-mac )

//l2 table configuration
//get configuration
//ipv4 multicast hash mode
l2-table get ip-mcast-mode


//l2 table configuration
//get configuration
//port moving configuration
//unknown sa configuration
//port configuration
//port list, <0-6>
//all
//action configuration
l2-table get ( port-move | unknown-sa ) port ( <PORT_LIST:ports> | all ) action

//l2 table configuration
//set configuration
//port moving configuration
//unknown sa configuration
//port configuration
//port list, <0-6>
//all
//action configuration
//copy to cpu
//drop & disable learning
//forward
//trap to cpu & disable learning
l2-table set ( port-move | unknown-sa ) port ( <PORT_LIST:ports> | all ) action ( copy-to-cpu | drop | forward | trap-to-cpu )

//l2 table configuration
//set configuration
//mac flush configuration
//unicast l2 entry
l2-table set flush mac-ucast

//l2 table configuration
