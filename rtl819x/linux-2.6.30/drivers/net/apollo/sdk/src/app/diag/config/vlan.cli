//vlan configuration
//destroy VLAN
//all VLAN
vlan destroy all

//vlan configuration
//destroy VLAN
//all VLAN
//set all port to untag member
vlan destroy all untag



//vlan configuration
//destroy VLAN
//vlan member configuration
//all 32 vlan member configuration
vlan destroy entry all

//vlan configuration
//destroy VLAN
//vlan member configuration
//vidx, <0~31>
vlan destroy entry <UINT:index>

//l2 table configuration
//destroy VLAN
//vid configuration
//vlan id, <0~4095>
vlan destroy vid <UINT:vid>

//vlan configuration
//get configuration
//accept frame type
//port configuration
//port list, <0-6>
//all port
vlan get accept-frame-type port ( <PORT_LIST:ports> | all ) 

//vlan configuration
//get configuration
//vlan tag real keep function
//egress port configuration
//ports, <0-6>
//all ports
//ingress port configuration
//ports, <0-6>
//all ports
//state of egress keep
vlan get egress-keep port ( <PORT_LIST:egr_ports> | all ) ingress-port ( <PORT_LIST:igr_ports> | all ) state

//vlan configuration
//get configuration
//ingress filter
//port configuration
//ports, <0-6>
//all ports
//state configuration
vlan get ingress-filter port ( <PORT_LIST:ports> | all ) state

//vlan configuration
//get configuration
//vlan member configuration
//vlan mbr index, <0~31>
vlan get entry <UINT:index>


//vlan configuration
//get configuration
//vlan member configuration
//all entry
vlan get entry all

//vlan configuration
//get configuration
//protocol VLAN configuration
//protocol group
//group index, <0~3>
vlan get protocol-vlan group <UINT:index> 

//vlan configuration
//set configuration
//config port based vid
//port configuration
//ports, <0-6>
//all ports
//vlan entry index 
vlan get pvid port ( <PORT_LIST:ports> | all ) vlan_index

//vlan configuration
//get configuration
//config dsl port based vid
//VC port configuration
//ports, <0-15>
//all ports
//vlan entry index 
vlan get vc_pvid port ( <PORT_LIST:ports> | dsl_all ) vlan_index

//vlan configuration
//get configuration
//config extention port based vid
//extention port configuration
//ports, <1-5>
//vlan entry index 
vlan get ext_pvid port <PORT_LIST:ports> vlan_index


//vlan configuration
//get configuration
//vlan function state
vlan get state 

//vlan configuration
//get configuration
//egress Tag mode
//port configuration
//ports, <0-6>
//all ports
vlan get tag-mode port ( <PORT_LIST:ports> | all ) 

//vlan configuration
//get configuration
//transparent function configuration
//state of vlan transparent
vlan get transparent state

//vlan configuration
//get configuration
//vlan treat configuration
//vid configuration
//vid 0
//vid 4095
//vlan treat type
vlan get vlan-treat vid ( 0 | 4095 ) type

//vlan configuration
//set configuration
//vlan entry
//vid configuration
//vlan id, <0~4095>
vlan get vlan vid <UINT:vid>

//vlan configuration
//set configuration
//accept frame type
//port configuration
//port list, <0-6>
//all port
//accept all type frame
//accept tag only frame
//accept untag only frame
//accept 1p and 1Q tag only frame
vlan set accept-frame-type port ( <PORT_LIST:ports> | all ) ( all | tagged-only | untagged-only | tagged-1p-1q-only )

//vlan configuration
//set configuration
//vlan tag real keep function
//egress port configuration
//ports, <0-6>
//all ports
//ingress port configuration
//ports, <0-6>
//all ports
//state of egress keep
//enable egress keep function
//disable egress keep function
vlan set egress-keep port ( <PORT_LIST:egr_ports> | all ) ingress-port ( <PORT_LIST:igr_ports> | all ) state ( enable | disable )

//vlan configuration
//set configuration
//ingress filter
//port configuration
//ports, <0-6>
//all ports
//state configuration
//enable function
//disable function
vlan set ingress-filter port ( <PORT_LIST:ports> | all ) state ( enable | disable )

//vlan configuration
//set configuration
//vlan member configuration
//vlan mbr index, <0~31>
//evid configuration
//enhanced VLAN id, <0~8191>
vlan set entry <UINT:index> evid <UINT:evid>

//vlan configuration
//set configuration
//vlan member configuration
//vlan mbr index, <0~31>
//FID configuration
//filter id, <0~15>
vlan set entry <UINT:index> filter-id <UINT:fid>

//vlan configuration
//set configuration
//vlan member configuration
//vlan mbr index, <0~31>
//set member port
//member ports, <0-6>
vlan set entry <UINT:index> member <PORT_LIST:member_ports>


//vlan configuration
//set configuration
//vlan member configuration
//vlan mbr index, <0~31>
//set dsl member port
//member ports, <0-15>
vlan set entry <UINT:index> dsl_member <PORT_LIST:dls_member_ports>


//vlan configuration
//set configuration
//vlan member configuration
//vlan mbr index, <0~31>
//set extention member port
//member ports, <0-5>
