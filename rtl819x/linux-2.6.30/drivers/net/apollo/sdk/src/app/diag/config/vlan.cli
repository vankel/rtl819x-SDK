//vid 4095
//vlan treat type
//tagging frame
//un-tagging frame
vlan set vlan-treat vid ( 0 | 4095 ) type ( tagging | un-tagging )

//vlan configuration
//set configuration
//vlan entry
//vid configuration
//vlan id, <0~4095>
//FID configuration
//filter id, <0~15>
vlan set vlan vid <UINT:vid> filter-id <UINT:fid>

//vlan configuration
//set configuration
//vlan entry
//vid configuration
//vlan id, <0~4095>
//hash mode
//ivl mode
//svl mode
vlan set vlan vid <UINT:vid> hash-mode ( ivl | svl ) 

//vlan configuration
//set configuration
//vlan member configuration
//vidx configuration
//vlan mbr index, <0~31>
//set member port
//member ports, <0-6>
//dsl configuration
//dsl vc list, <0-15>
//all
//ext configuration
//ext port list, <0-5>
//all
vlan set vlan vid <UINT:vid> member <PORT_LIST:member_ports> dsl ( <PORT_LIST:dsl> | dsl_all ) ext ( <PORT_LIST:ext> | ext_all ) 

//vlan configuration
//set configuration
//vlan entry
//vid configuration
//vlan id, <0~4095>
//set member port
//member ports, <0-6>
//set tag ports
//tag member ports, <0-6>
