//dsl configuration
//dsl vc list, <0-15>
//all
//ext configuration
//ext port list, <0-5>
//all
vlan set vlan vid <UINT:vid> member <PORT_LIST:member_ports> tag_mbr ( <PORT_LIST:tagged_ports> | none ) dsl ( <PORT_LIST:dsl> | dsl_all ) ext ( <PORT_LIST:ext> | ext_all ) 

