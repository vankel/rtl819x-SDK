.PHONY: $(TARGET)


$(TARGET): $(ALL_OBJ)
	$(AR) rcs $@ $(ALL_OBJ)

%.o: %.c
	$(CC) -c $(EXTRA_CFLAGS) $(CFLAGS) -o $@ $<





romfs:

clean:
	rm -rf $(TARGET) $(ALL_OBJ)

