EXTRA_CFLAGS += -I$(SDK_INCLUDE) -I$(SYS_INCLUDE) -I$(DRV_INCLUDE)


CFLAGS +=     -Wcomment \
              -Winit-self \
              -Wmissing-braces \
              -Wparentheses \
              -Wreturn-type \
              -Wunused-parameter \
              -Wunused-variable \
              -Wunused-value \
              -Wdeclaration-after-statement \
              -Wmissing-prototypes \
              -Wimplicit \
               -g -O2


